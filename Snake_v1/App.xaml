<Application x:Class="Snake_v1.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Snake_v1"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor">#FF00FA9A</SolidColorBrush>
        <SolidColorBrush x:Key="GridBackgroundColor">#FF008080</SolidColorBrush>
        <SolidColorBrush x:Key="GridLineColor">#FF40E0D0</SolidColorBrush>
        <SolidColorBrush x:Key="TextColor">#FFDAA520</SolidColorBrush>
        <SolidColorBrush x:Key="OverlayColor">#5F000000</SolidColorBrush>
        <FontFamily x:Key="MainFont">Assets/#Droid Sans Mono</FontFamily>

    </Application.Resources>
</Application>
